{
    "name": "Apache Camel Karavan",
    "image": "maven:3.9.3-eclipse-temurin-17",
    "workspaceFolder": "/workspaces/camel-karavan",
    "mounts": [
      "source=${localEnv:HOME}/.m2,target=/home/karavan/.m2,type=bind,consistency=cached"
    ],
    "customizations": {
      "vscode": {
        "settings": {
          "java.autobuild.enabled": false
        },
        "extensions": [
          "vscjava.vscode-java-pack"
        ]
      }
    },
    "features": {
      "ghcr.io/devcontainers/features/common-utils:2.2.0": {
        "installZsh": "true",
        "username": "karavan",
        "uid": "1000",
        "gid": "1000",
        "upgradePackages": "false"
      },
      "ghcr.io/devcontainers/features/node:1.3.1": {
        "version": "18.18.2"
      },
      "ghcr.io/devcontainers/features/docker-in-docker:2.7.0": {}
    },
    "forwardPorts": [8080],
    "remoteUser": "karavan",
    "initializeCommand": "mkdir -p ${localEnv:HOME}/.m2",
    "postCreateCommand": "bash .devcontainer/postCreateCommand.sh"
  }
  